{
  "name": "wisebuild-frontend",
  "version": "1.0.0",
  "description": "WiseBuild Frontend project",
  "author": "jwzhao@wise2c.com",
  "private": true,
  "scripts": {
    "dev": "better-npm-run dev",
    "build": "better-npm-run build",
    "build-ci": "better-npm-run build-ci",
    "unit": "better-npm-run unit",
    "e2e": "better-npm-run e2e",
    "test": "better-npm-run test",
    "lint": "better-npm-run lint"
  },
  "betterScripts": {
    "dev": {
      "command": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js"
    },
    "build": {
      "command": "node --trace-deprecation build/build.js"
    },
    "build-ci": {
      "command": "node build/workflow.js"
    },
    "unit": {
      "command": "karma start test/unit/karma.conf.js --single-run"
    },
    "e2e": {
      "command": "node test/e2e/runner.js"
    },
    "test": {
      "command": "npm run unit && npm run e2e"
    },
    "lint": {
      "command": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs"
    }
  },
